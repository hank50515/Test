<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="NHinernateUnit" namespace="NHinernateUnit" xmlns="urn:nhibernate-mapping-2.2">
  <class name="ProductListPriceHistory" table="ProductListPriceHistory">
    <id name="ProductID" type="Int32">
      <column name="ProductID" not-null="true" sql-type="INTEGER" />
      <generator class="identity" />
    </id>
    <property name="StartDate" type="datetime">
      <column name="StartDate" not-null="false" length="20"/>
    </property>
    <property name="EndDate" type="datetime">
      <column name="EndDate" not-null="false" length="20" />
    </property>
	<property name="ListPrice" type="money">
      <column name="ListPrice" not-null="false"/>
    </property>
	<property name="ModifiedDate" type="datetime">
      <column name="ModifiedDate" not-null="false" length="20" />
    </property>
	<property name="ModifiedDate" type="datetime">
      <column name="ModifiedDate" not-null="false" length="20" />
    </property>
  </class>
</hibernate-mapping>